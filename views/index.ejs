<%- include("partials/header.ejs")%>

<p class="fs-1"><%=title%></h1>
<p><%=content%></p>

<% if(locals.posts) { %>
  <% posts.forEach(function(post){ %>
    <p class="fs-1"><%=post.title%></p>
    <p class="fs-6"><%=post.text%></p>
    <% }) %> 
<% } else { %>
  <p>No posts to display</p>
<% } %>

<%- include("partials/footer.ejs")%>
